<myththeme>
    <font name="basefont"  face="Trebuchet Ms">
      <color>#ffffff</color>
      <dropcolor>#3B4045</dropcolor>
      <size>20</size>
      <size:small>18</size:small>
      <size:big>22</size:big>
      <shadow>2,2</shadow>
      <bold>yes</bold>
    </font>
   <font name="title" base="basefont" face="Arial Black">
      <!--<color>#ffffdd</color>-->
      <color>#DEEAF9</color>
      <dropcolor>#000000</dropcolor>
      <size>24</size>
      <size:big>24</size:big>
      <size:small>18</size:small>
    </font>
    
    <font name="subtitle" base="basefont"  face="Arial Black">
      <color>#ffffdd</color>
      <size>22</size>
      <size:big>24</size:big>
      <size:small>20</size:small>
    </font>
    
    <font name="info" base="basefont">
      <shadow>2,2</shadow>
    </font>
    
    <font name="metadata" base="info">
      <color>#ffffdd</color>
    </font>
    
    <font name="help" base="info">
      <shadow>2,2</shadow>
      <color>#aaaaee</color>
    </font>
    
    <font name="description" base="info">
      <shadow>2,2</shadow>
    </font>
    
    <font name="small-info" base="info">
      <size>14</size>
      <shadow>1,1</shadow>
      <size:small>12</size:small>
      <size:big>17</size:big>
    </font>

    <font name="list-active" base="basefont" face="Freesans">
      <size>18</size>
      <size:small>14</size:small>
      <size:big>20</size:big>
      <shadow>2,2</shadow>
      <color>#ddddff</color>
    </font>

    <font name="list-inactive" base="list-active">
      <color>#aaaaee</color>
    </font>
  
    <font name="list-record" base="list-active">
      <color>#33ff33</color>
    </font>
    
    <font name="list-norecord" base="list-active">
      <color>#999999</color>
    </font>

    <font name="list-conflict"  base="list-active">
      <color>#dddd33</color>
    </font>
    
    <font name="list-selected"  base="list-active">
      <color>#ffffff</color>
    </font>

    <font name="list-selectable" base="list-active">
        <color>#ddddff</color>
    </font>

        
    <font name="recstatus" base="info">
      <color>#ffff33</color>
      <dropcolor>#000000</dropcolor>
      <shadow>2,2</shadow>
    </font>

  <background style="normal">
    <image>background.jpg</image>

    <buttonarea>30,115,800,600</buttonarea>
    <visiblerowlimit>7</visiblerowlimit>
    <columns>1</columns>
  </background>

  <titles>
    <position>20,10</position>
    <image mode="TV">title/title_tv.png</image>
    <image mode="MAIN">title/title_main.png</image>
    <image mode="MUSIC">title/title_music.png</image>
    <image mode="GALLERY">title/title_gallery.png</image>
    <image mode="VIDEO">title/title_video.png</image>
    <image mode="SCHEDULE">title/title_schedule.png</image>
    <image mode="SEARCH">title/title_search.png</image>
    <image mode="SETUP">title/title_setup.png</image>
    <image mode="MEDIA_SETUP">title/media_setup.png</image>
    <image mode="INFO_SETUP">title/title_info_setup.png</image>
    <image mode="TVSETUP">title/title_tv_setup.png</image>
    <image mode="DVD">title/title_dvd.png</image>
    <image mode="DVD_SETTINGS">title/title_dvd_setup.png</image>
    <image mode="MUSIC_SETTINGS">title/title_music_setup.png</image>
    <image mode="VIDEO_SETTINGS">title/title_video_setup.png</image>
    <image mode="GAME_SETTINGS">title/title_game_setup.png</image>
    <image mode="LIBRARY">title/title_library.png</image>
    <image mode="MANAGE_RECORDINGS">title/title_manage_recordings.png</image>
    <image mode="INFO">title/title_info_center.png</image>
    <image mode="OPTICAL_DISK">title/title_optical_disks.png</image>
    <image mode="UTIL">title/title_util.png</image>
  </titles>

  <genericbutton>
    <watermarkposition>880,115</watermarkposition> <!-- 20px right margin -->
    <normal>button_off.png</normal>
    <active>black_button_on.png</active>
    <text>
      <area>50,8,3,0</area>
      <color>#DEEAF9</color>
<!--       <color>white</color> -->
      <fontname>Trebuchet Ms</fontname>
      <fontsize>28</fontsize>
      <bold>yes</bold>
      <italics>no</italics>
      <centered>no</centered>
      <outline>
         <color>black</color>
         <size>2</size>
      </outline>
      <shadow>
         <color>black</color>
         <offset>2,2</offset>
         <alpha>128</alpha>
      </shadow>
    </text>
    <activetext>
      <area>50,8,3,0</area>
<!--       <color>#0086FF</color> -->
      <color>#3C5574</color>
      <fontname>Trebuchet Ms</fontname>
      <fontsize>28</fontsize>
      <bold>yes</bold>
      <italics>no</italics>
      <centered>no</centered>
      <outline>
         <color>black</color>
         <size>2</size>
      </outline>
      <!--<shadow>
         <color>black</color>
         <offset>3,3</offset>
         <alpha>128</alpha>
      </shadow>-->
    </activetext>
  </genericbutton>
  

  <uparrow>
    <image>shared/up_arrow.png</image>
    <position>65,120</position>
  </uparrow>

  <downarrow>
    <image>shared/down_arrow.png</image>
    <position>65,460</position>
  </downarrow>

  
