<myththeme>

  <background style="normal">
    <image>background.jpeg</image>
    <buttonarea>115,115,410,390</buttonarea>
    <visiblerowlimit>7</visiblerowlimit>
  </background>

  <titles>
    <position>20,20</position>
    <image mode="TV">title/title_tv.png</image>
    <image mode="MAIN">title/title_main.png</image>
    <image mode="MUSIC">title/title_music.png</image>
    <image mode="GALLERY">title/title_gallery.png</image>
    <image mode="VIDEO">title/title_video.png</image>
    <image mode="SCHEDULE">title/title_schedule.png</image>
    <image mode="SEARCH">title/title_search.png</image>
    <image mode="SETUP">title/title_setup.png</image>
    <image mode="TVSETUP">title/title_tv_settings.png</image>
    <image mode="DVD">title/title_dvd.png</image>
    <image mode="DVD_SETTINGS">title/title_dvd_setup.png</image>
    <image mode="MUSIC_SETTINGS">title/title_music_setup.png</image>
    <image mode="VIDEO_SETTINGS">title/title_video_setup.png</image>
    <image mode="GAME_SETTINGS">title/title_game_setup.png</image>
    <image mode="LIBRARY">title/title_library.png</image>
    <image mode="MANAGE_RECORDINGS">title/title_manage_recordings.png</image>
    <image mode="INFO">title/title_info_center.png</image>
    <image mode="OPTICAL_DISK">title/title_optical_disks.png</image>
    <image mode="UTIL">title/title_util.png</image>
  </titles>

  <genericbutton>
    <watermarkposition>524,105</watermarkposition> <!-- 20px right margin -->
    <normal>text-blank.png</normal>
    <active>text-blank.png</active>
    <text>
    <area>5,3,3,3</area>
    <color>white</color>
      <fontname>Arial</fontname>
      <fontsize>28</fontsize>
      <bold>yes</bold>
      <italics>no</italics>
      <centered>no</centered>
      <outline>
         <color>black</color>
         <size>2</size>
      </outline>
      <!--<shadow>
         <color>black</color>
         <offset>2,2</offset>
         <alpha>128</alpha>
      </shadow>-->
    </text>
    <activetext>
      <area>5,3,3,3</area>
      <color>#0086FF</color>
      <fontname>Arial</fontname>
      <fontsize>28</fontsize>
      <bold>yes</bold>
      <italics>no</italics>
      <centered>no</centered>
      <outline>
         <color>black</color>
         <size>3</size>
      </outline>
      <!--<shadow>
         <color>black</color>
         <offset>3,3</offset>
         <alpha>128</alpha>
      </shadow>-->
    </activetext>
  </genericbutton>
  

  <uparrow>
    <image>arrow_up.png</image>
    <position>65,120</position>
  </uparrow>

  <downarrow>
    <image>arrow_down.png</image>
    <position>65,460</position>
  </downarrow>

  
